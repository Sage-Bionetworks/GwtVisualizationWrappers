<?xml version="1.0" encoding="UTF-8"?>

<module rename-to="CytoscapeJs">
    <public path="client/cytoscape/resource">
        <include name="css/*.css"/>
        <include name="js/*.js"/>
        <include name="fonts/*"/>
    </public>
    
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.user.UserAgent"/>
    <inherits name="com.google.gwt.i18n.I18N"/>
	
	<entry-point class="org.gwtvisualizationwrappers.client.cytoscape.CytoscapeJsEntryPoint" />
	
    <source path="client/cytoscape" />
    <source path="shared" />
</module>
